console.log('Hello Node.js')
const moneyRate = 2;
let currentMoney = 0;
let days = 365;

currentMoney = days * moneyRate + currentMoney;
console.log('-------');
console.log(currentMoney);
console.log('-------');
console.log('Statistics:');
console.log('Days:' + days);
console.log('Money Rate:' + moneyRate);
console.log('-------');

